<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Resources\add_element.png" />
    <None Remove="Resources\add_node.png" />
    <None Remove="Resources\box_3D.png" />
    <None Remove="Resources\box_front.png" />
    <None Remove="Resources\box_side.png" />
    <None Remove="Resources\box_top.png" />
    <None Remove="Resources\display_all.png" />
    <None Remove="Resources\FEM_Calculator_icon_2048.png" />
